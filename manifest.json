{
   "background": {
      "persistent": true,
      "scripts": [ "js/eventPage.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "main.html"
   },
   "description": "Lets you login Force.com with 1 click.",
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "48": "img/icon_48.png"
   },
   "incognito": "spanning",
   "manifest_version": 2,
   "name": "Force.com LOGINSK2",
   "omnibox": {
      "keyword": "sf"
   },
   "options_page": "config.html",
   "permissions": [ "storage", "unlimitedStorage", "https://login.salesforce.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.7.7"
}
